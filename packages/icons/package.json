{
  "name": "@tourepedia/icons",
  "version": "0.4.1",
  "description": "Icons for Tourepedia Apps",
  "repository": "https://github.com/tourepedia/tp-ui/packages/icons",
  "author": "Sudhir <sudhir.mitharwal@tourepedia.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "scripts": {
    "prebuild:js": "svgr -d src/icons src/svgs --ext tsx && node ./add-imports && mkdir -p styles && cp src/icon.css styles/icon.css",
    "build:js": "node ../../shared/build",
    "js:watch": "chokidar \"./src/svgs/*.(svg)\" -c \"npm run build:js\"",
    "build": "npm run build:js",
    "build:watch": "npm run js:watch"
  },
  "files": [
    "lib",
    "es",
    "styles",
    "types"
  ],
  "gitHead": "7915c06b02d5fe40f2fc99a2bce33ef7a5ccbf01",
  "devDependencies": {
    "@svgr/cli": "^4.3.0"
  }
}
